<h1>Listing tagsubs</h1>

<table>
  <tr>
    <th>Home</th>
    <th>Tag</th>
  </tr>

<% for tagsub in @tagsubs %>
  <tr>
    <td><%=h tagsub.home_id %></td>
    <td><%=h tagsub.tag_id %></td>
    <td><%= link_to 'Show', tagsub %></td>
    <td><%= link_to 'Edit', edit_tagsub_path(tagsub) %></td>
    <td><%= link_to 'Destroy', tagsub, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tagsub', new_tagsub_path %>
