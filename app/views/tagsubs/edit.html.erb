<h1>Editing tagsub</h1>

<%= form_for(@tagsub) do |f| %>
  <%= render "shared/error_messages", :target => @tagsub %>

  <p>
    <%= f.label :home_id %><br />
    <%= f.text_field :home_id %>
  </p>
  <p>
    <%= f.label :tag_id %><br />
    <%= f.text_field :tag_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @tagsub %> |
<%= link_to 'Back', tagsubs_path %>
